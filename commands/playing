var exec = require('child-process').exec

function playing(){
  var track = ""
  for(var i in process.argv){
    if (i >= 2){
      if(process.argv[i] == "spotify"){
        track = execSync("sh /Users/codycallahan/.config/ubersicht/widgets/bar/commands/spotify").output;
      } else {
        track = execSync("cat /Users/codycallahan/.config/ubersicht/widgets/bar/playing/soundcloud").toString();
      }
      if(!!track) {
        return { track: track, source: process.argv[i] };
      }
    }
  }
};

console.log("test");
